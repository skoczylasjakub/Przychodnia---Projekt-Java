<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.DoctorController">
   <children>
      <TableView fx:id="tableVisit" layoutX="243.0" layoutY="30.0" prefHeight="318.0" prefWidth="227.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Imie" />
          <TableColumn prefWidth="75.0" text="Nazwisko" />
            <TableColumn prefWidth="75.0" text="Data" />
        </columns>
      </TableView>
      <Button fx:id="clearData" layoutX="133.0" layoutY="322.0" mnemonicParsing="false" onAction="#clearDataClick" text="Clean" />
      <Button fx:id="searchVisit" layoutX="40.0" layoutY="322.0" mnemonicParsing="false" onAction="#searchVisitClick" text="Szukaj" />
      <ComboBox fx:id="statusVisit" layoutX="40.0" layoutY="59.0" prefWidth="150.0" promptText="Status wizyt" />
      <Button fx:id="exitApp" layoutX="525.0" layoutY="17.0" mnemonicParsing="false" onAction="#onExitAppClick" text="Wyjscie" />
   </children>
</AnchorPane>
